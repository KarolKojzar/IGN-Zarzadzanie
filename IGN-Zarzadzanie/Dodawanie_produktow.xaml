        <TextBox Height="23" HorizontalAlignment="Left" Margin="10,234,0,0" Name="wprowadz_cena_detal_textBox" VerticalAlignment="Top" Width="120" PreviewTextInput="wprowadz_cena_detal_textBox_PreviewTextInput" />
