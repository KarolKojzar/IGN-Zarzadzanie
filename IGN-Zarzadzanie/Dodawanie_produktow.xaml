        <TextBox Height="23" HorizontalAlignment="Left" Margin="10,108,0,0" Name="wprowadz_cena_agencyjna_EKO_textBox" VerticalAlignment="Top" Width="121" PreviewTextInput="wprowadz_cena_agencyjna_EKO_textBox_PreviewTextInput"/>
