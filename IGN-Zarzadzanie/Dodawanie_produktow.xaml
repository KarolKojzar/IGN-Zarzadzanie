        <Button Content="Zatwierdź" Height="23" HorizontalAlignment="Left" Margin="191,165,0,0" Name="Zatwierdz_BTN" VerticalAlignment="Top" Width="75" Click="Zatwierdz_BTN_Click" Background="#FFE4FFDE" />
        <Button Content="Anuluj" Height="23" HorizontalAlignment="Left" Margin="191,234,0,0" Name="Anuluj_BTN" VerticalAlignment="Top" Width="75" Click="Anuluj_BTN_Click" Background="#FFE4FFDE" />
